{
    "name": "dilovod/php-sdk",
    "description": "PHP SDK for integration with Dilovod API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dilovod",
        "api",
        "sdk",
        "accounting",
        "ukraine"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dilovod\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dilovod\\Sdk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

